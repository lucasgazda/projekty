
public class Czytelnik {
	
	private String imie;
	private String nazwisko;
	private int wiek;
	private double wysokoscKary;
	
	
	public String getImie() {
		return imie;
	}
	
	public void setImie(String imie) {
		this.imie = imie;
	}
	
	public String getNazwisko() {
		return nazwisko;
	}
	
	public void setNazwisko(String nazwisko) {
		this.nazwisko = nazwisko;
	}
	
	public int getWiek() {
		return wiek;
	}
	 
	public void setWiek(int wiek) {
		this.wiek = wiek;
	}
	 
	public double getWysokoscKary() {
		return wysokoscKary;
	}
	 
	public void setWysokoscKary(double wysokoscKary) {
		this.wysokoscKary = wysokoscKary;
	}

	public Czytelnik() {
		
	}

	public Czytelnik(String imie, String nazwisko, int wiek, double wysokoscKary) {
		this.imie = imie;
		this.nazwisko = nazwisko;
		this.wiek = wiek;
		this.wysokoscKary = wysokoscKary;
	}
	
	@Override
		public String toString() {
			String text = "";
			text += "\n";
			text += "IMIE = " + imie + "\n";
			text += "NAZWISKO = " + nazwisko + "\n";
			text += "WIEK = " + wiek + "\n";
			text += "WYSOKOSC KARY = " + wysokoscKary + "\n";
			return text;
		}

	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((imie == null) ? 0 : imie.hashCode());
		result = prime * result + ((nazwisko == null) ? 0 : nazwisko.hashCode());
		result = prime * result + wiek;
		long temp;
		temp = Double.doubleToLongBits(wysokoscKary);
		result = prime * result + (int) (temp ^ (temp >>> 32));
		return result;
	}

	
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Reader other = (Reader) obj;
		if (imie == null) {
			if (other.imie != null)
				return false;
		} else if (!imie.equals(other.imie))
			return false;
		if (nazwisko == null) {
			if (other.nazwisko != null)
				return false;
		} else if (!nazwisko.equals(other.nazwisko))
			return false;
		if (wiek != other.wiek)
			return false;
		if (Double.doubleToLongBits(wysokoscKary) != Double.doubleToLongBits(other.wysokoscKary))
			return false;
		return true;
	}
}
